!!!5

%html{html_attrs}
  %head 
    %title TeachAssist
    = stylesheet_link_tag "application" 
    = javascript_include_tag "application"
    = csrf_meta_tag
  
  %body 
    - if current_account
      %a#link-logout{ :href => '/logout' } Sign Out
    - else 
      %a#link-login{ :href => '/login' } Sign In
    
    = yield
  

