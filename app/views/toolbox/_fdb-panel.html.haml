
#fdb-panel.no-remove
  .row-fluid
    .question.span7= "An honest effort ?"
    .span4.offset1
      - Requirement.honest.each do |m|
        .row-fluid.hide.requirement{ :marker => m.id }
          .row.bottomline= "#{m.bottomline}"
          .row.text= "#{m.text}"
  .row-fluid
    .question.span7= "Is whats written relevant to the question ?"
    .span4.offset1
      - Requirement.cogent.each do |m|
        .row-fluid.hide.requirement{ :marker => m.id }
          .row.bottomline= "#{m.bottomline}"
          .row.text= "#{m.text}"
  .row-fluid
    .question.span7= "Is it also complete ?"
    .span4.offset1
      - Requirement.complete.each do |m|
        .row-fluid.hide.requirement{ :marker => m.id }
          .row.bottomline= "#{m.bottomline}"
          .row.text= "#{m.text}"
  .row-fluid
    .question.span7= "Anything else ?"
    .span4.offset1
      - Requirement.other.each do |m|
        .row-fluid.hide.requirement{ :marker => m.id }
          .row.bottomline= "#{m.bottomline}"
          .row.text= "#{m.text}"
